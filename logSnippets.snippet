<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>logd</Title>
			<Shortcut>logd</Shortcut>
			<Description>Code snippet for Program.Log.Debug</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="false">
					<ID>Logger</ID>
					<Function>SimpleTypeName(global::Webserver.Program.Log)</Function>
				</Literal>
				<Literal Editable="true">
					<ID>Message</ID>
					<ToolTip>Write a "debug" log message.</ToolTip>
					<Default>$""</Default>
				</Literal>
			</Declarations>
			<Code Language="CSharp"><![CDATA[$Logger$.Debug($Message$);]]></Code>
		</Snippet>
	</CodeSnippet>
	
