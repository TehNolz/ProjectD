	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>log</Title>
			<Shortcut>log</Shortcut>
			<Description>Code snippet for Program.Log.Write with a custom log level</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="false">
					<ID>Logger</ID>
					<Function>SimpleTypeName(global::Webserver.Program.Log)</Function>
				</Literal>
				<Object Editable="true">
					<ID>Level</ID>
					<Type>Logging.Level</Type>
					<ToolTip>Choose or create a custom logging level</ToolTip>
					<Default>level</Default>
				</Object>
				<Literal Editable="true">
					<ID>Message</ID>
					<ToolTip>Write a log message.</ToolTip>
					<Default>$""</Default>
				</Literal>
			</Declarations>
			<Code Language="CSharp"><![CDATA[$Logger$.Write($Level$, $Message$);]]></Code>
		</Snippet>
	</CodeSnippet>
