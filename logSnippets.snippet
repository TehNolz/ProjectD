	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>logfe</Title>
			<Shortcut>logfe</Shortcut>
			<Description>Code snippet for Program.Log.Fatal with an exception message</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal Editable="false">
					<ID>Logger</ID>
					<Function>SimpleTypeName(global::Webserver.Program.Log)</Function>
				</Literal>
				<Literal Editable="true">
					<ID>Message</ID>
					<ToolTip>Write a "fatal" log message.</ToolTip>
					<Default>$""</Default>
				</Literal>
				<Object Editable="true">
					<ID>Exception</ID>
					<ToolTip>Choose an exception</ToolTip>
					<Type>System.Exception</Type>
					<Default>e</Default>
				</Object>
			</Declarations>
			<Code Language="CSharp"><![CDATA[$Logger$.Fatal(string.Concat($Message$, ": ", $Exception$.Message), $Exception$);]]></Code>
		</Snippet>
	</CodeSnippet>
	
